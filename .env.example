# Frontend Environment Variables
VITE_API_URL=http://localhost:8000

# Backend Environment Variables (for Python backend)
# AWS Configuration
AWS_DEFAULT_REGION=us-west-2
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Bedrock Model Configuration
BEDROCK_MODEL_ID=us.anthropic.claude-3-7-sonnet-20250219-v1:0
BEDROCK_REGION=us-west-2

# Database Configuration (if using external database)
DATABASE_URL=postgresql://user:password@localhost:5432/fraud_detection

# Redis Configuration (for caching and real-time features)
REDIS_URL=redis://localhost:6379

# Application Configuration
DEBUG=true
LOG_LEVEL=INFO
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Security Configuration
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here

# External Services (optional)
EXTERNAL_FRAUD_API_URL=https://api.frauddetection.com
EXTERNAL_FRAUD_API_KEY=your_external_api_key
